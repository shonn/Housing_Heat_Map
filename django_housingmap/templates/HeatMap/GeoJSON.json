{% autoescape off %}
{% if shapes %}
{
  "type": "FeatureCollection",
  "features": [
    {% for shape in shapes %}
    	{ "type": "Feature",
    		"geometry": {{shape.geom.geojson}},
    		"properties": {"name": "{{shape.zcta5ce10}}"}
    	}{% if not forloop.last%},{% endif %}
    {% endfor %}
    ]}
{% else %}
    <p>No shapes are available.</p>
{% endif %}
{% endautoescape %}