{% autoescape off %}
{% if data %}
{
  "type": "FeatureCollection",
  "features": [
    {% for datum in data %}
    	{ "type": "Feature",
    		"geometry": {{datum.shape}},
    		"properties": {"name": "{{datum.name}}",
    						"value":"{{datum.value}}",
    						"value-type":"{{datum.type}}"}
    	}{% if not forloop.last%},{% endif %}
    {% endfor %}
    ]}
{% else %}
    <p>No shapes are available.</p>
{% endif %}
{% endautoescape %}